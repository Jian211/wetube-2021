extends base.pug


block contents 
    h1 밑에 비디오나와요
    video(src="/"+video.videoUrl , controls) 
    div
        h2=video.title
        p=video.description
        p=video.hashtags
        div
            small=video.createdAt
        a(href=`${video.id}/edit`) Edit Video &rarr;
        a(href=`${video.id}/delete`) Delete Video &rarr;
